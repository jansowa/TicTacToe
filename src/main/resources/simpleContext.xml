<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="board" class="game.SimpleBoard"></bean>
	
	<bean id="view" class="game.SimpleGameView" autowire="constructor">
	</bean>
	
	<bean id="ai" class="game.BrutalAI" autowire="constructor">
	</bean>
	
	<bean id="dao" class="game.SimpleGameDAO" autowire="constructor">
	</bean>
	
	<bean id="gameApp" class="game.GameApp" autowire="constructor">
	</bean>
</beans>
